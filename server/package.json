{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run tsc --project tsconfig.compile.json",
    "build:w": "tsc --project tsconfig.compile.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "seed": "ts-node prisma/seed/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.9.1",
    "@tsed/ajv": "^6.101.0",
    "@tsed/common": "^6.101.0",
    "@tsed/core": "^6.101.0",
    "@tsed/di": "^6.101.0",
    "@tsed/exceptions": "^6.101.0",
    "@tsed/json-mapper": "^6.101.0",
    "@tsed/passport": "^6.101.0",
    "@tsed/platform-express": "^6.101.0",
    "@tsed/schema": "^6.101.0",
    "@tsed/swagger": "^6.101.0",
    "ajv": "^8.10.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "method-override": "^3.0.0",
    "nodemailer": "^6.7.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "rand-token": "^1.0.1",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@tsed/cli-plugin-eslint": "3.17.0",
    "@tsed/cli-plugin-jest": "3.17.0",
    "@tsed/cli-plugin-passport": "3.17.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.4.0",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.16",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prisma": "^3.9.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default",
    "architecture": "default"
  }
}
